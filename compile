#/bin/sh

set -e

echo "Compile"

mkdir -p target/classes target/test-classes

javac -encoding "UTF-8" -d target/classes -classpath "lib/reflections-0.9.9-RC2.jar;target/classes;target/test-classes/" src/main/java/com/baldurtech/*.java

javac -encoding "UTF-8" -d target/test-classes -classpath "lib/reflections-0.9.9-RC2.jar;target/classes;target/test-classes/" src/test/java/com/baldurtech/*.java

echo "Compile done."


